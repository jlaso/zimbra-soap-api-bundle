<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="jlaso_zimbra_soap_api.account_admin" class="Jlaso\ZimbraSoapApiBundle\Service\Admin\AccountAdmin">
            <argument>%jlaso_zimbra_soap_api%</argument>
        </service>

        <service id="jlaso_zimbra_soap_api.domain_admin" class="Jlaso\ZimbraSoapApiBundle\Service\Admin\DomainAdmin">
            <argument>%jlaso_zimbra_soap_api%</argument>
        </service>

        <service id="jlaso_zimbra_soap_api.alias_admin" class="Jlaso\ZimbraSoapApiBundle\Service\Admin\AliasAdmin">
            <argument>%jlaso_zimbra_soap_api%</argument>
        </service>

        <service id="jlaso_zimbra_soap_api.cos_admin" class="Jlaso\ZimbraSoapApiBundle\Service\Admin\CosAdmin">
            <argument>%jlaso_zimbra_soap_api%</argument>
        </service>

        <service id="jlaso_zimbra_soap_api.mail_admin" class="Jlaso\ZimbraSoapApiBundle\Service\Admin\MailAdmin">
            <argument>%jlaso_zimbra_soap_api%</argument>
        </service>

    </services>
</container>
